<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools" tools:ignore="ImpliedQuantity, ExtraTranslation, MissingTranslation">
    <!-- Title of the Rename Device Dialog -->
    <string name="device_center_rename_device_dialog_title">Rename device</string>
    <!-- The name of the Positive Button for the Rename Device Dialog -->
    <string name="device_center_rename_device_dialog_positive_button">Rename</string>
    <!-- The name of the Header in Device Center which signifies the User's Device that is currently in use -->
    <string name="device_center_list_view_item_header_this_device">This device</string>
    <!-- The name of the Header in Device Center which signifies the User's Devices that are not currently in use  -->
    <string name="device_center_list_view_item_header_other_devices">Other devices</string>
    <!-- The name of the Device or Folder Status when no matching Status is found-->
    <string name="device_center_list_view_item_status_unknown_status">Unknown status</string>
    <!-- The name of the Device if the SDK cannot retrieve the name for that Device -->
    <string name="device_center_list_view_item_title_unknown_device">Unknown device</string>
    <!-- The name of the Device or Folder Status which signifies that it is Up to Date -->
    <string name="device_center_list_view_item_status_up_to_date">Up to date</string>
    <!-- The name of the Device or Folder Status which signifies that it is currently updating its backup contents -->
    <string name="device_center_list_view_item_status_updating">Updating&#8230;</string>
    <!-- The name of the Device or Folder Status which signifies that it is currently updating its backup contents with a percentage completion -->
    <string name="device_center_list_view_item_status_updating_with_progress">%1$d%% Updating&#8230;</string>
    <!-- The name of the Device or Folder Status which signifies that it is disabled -->
    <string name="device_center_list_view_item_status_disabled">Disabled</string>
    <!-- The name of the Device or Folder Status which signifies that the backup procedure has been paused -->
    <string name="device_center_list_view_item_status_paused">Paused</string>
    <!-- The name of the Device or Folder Status which signifies that an error has occurred -->
    <string name="device_center_list_view_item_status_error">Error</string>
    <!-- The title of the Device Center feature -->
    <string name="device_center_top_app_bar_title">Device centre</string>
    <!-- The name of one of the Device Center Bottom Sheet items which takes the User to the Camera uploads feature when clicked -->
    <string name="device_center_bottom_sheet_item_camera_uploads">Camera uploads</string>
    <!-- The enabled Camera uploads status that is shown alongside the "Camera uploads" Device Center Bottom Sheet item -->
    <string name="device_center_bottom_sheet_item_camera_uploads_enabled_status">Enabled</string>
    <!-- The disabled Camera uploads status that is shown alongside the "Camera uploads" Device Center Bottom Sheet item -->
    <string name="device_center_bottom_sheet_item_camera_uploads_disabled_status">Disabled</string>
    <!-- The name of one of the Device Center Bottom Sheet items that when clicked, opens the "Rename device" Dialog to let the User rename the selected Device -->
    <string name="device_center_bottom_sheet_item_rename">Rename</string>
    <!-- The name of one of the Device Center Bottom Sheet items that when clicked, takes the User to the Node Information screen to view more information about the Device or Device Folder -->
    <string name="device_center_bottom_sheet_item_info">Info</string>
    <!-- The name of the Device or Folder Sub State when its code is STORAGE_OVERQUOTA -->
    <string name="device_center_list_view_item_sub_state_storage_overquota">Storage quota limit reached.</string>
    <!-- The name of the Device or Folder Sub State when its code is ACCOUNT_BLOCKED -->
    <string name="device_center_list_view_item_sub_state_account_blocked">Unable to sync or back up this folder as the account has been blocked</string>
    <!-- The name of the Device or Folder Sub State when its code is LOGGED_OUT -->
    <string name="device_center_list_view_item_sub_state_logged_out">Sync or backup has been stopped as you appear to have logged out of the desktop app. Log back in via the desktop app, and resume the sync or backup.</string>
    <!-- The name of the Device or Folder Sub State when its code is INSUFFICIENT_DISK_SPACE -->
    <string name="device_center_list_view_item_sub_state_insufficient_disk_space">Insufficient space for download.</string>
    <!-- The Error Message shown in the Rename Device Dialog when the User submits an empty Device Name -->
    <string name="device_center_rename_device_dialog_error_message_empty_device_name">Enter a device name.</string>
    <!-- The Error Message shown in the Rename Device Dialog when the User submits a new Device Name that already exists in the current list of Devices -->
    <string name="device_center_rename_device_dialog_error_message_name_already_exists">A device with this name already exists. Enter a different name.</string>
    <!-- The Error Message shown in the Rename Device Dialog when the User submits a new Device Name with invalid characters -->
    <string name="device_center_rename_device_dialog_error_message_invalid_characters">The following characters are not allowed: %1$s</string>
    <!-- The Snackbar message to inform the User that the Device has been successfully renamed -->
    <string name="device_center_snackbar_message_rename_device_successful">Device renamed</string>
    <!-- The Error Message shown in the Rename Device Dialog when the User submits a new Device Name whose length is greater than 32 characters -->
    <string name="device_center_rename_device_dialog_error_message_maximum_character_length_exceeded">Maximum 32 characters</string>
    <!-- Label of the top app bar menu option of selected device in Device Center which takes the user to the Camera uploads feature settings -->
    <string name="device_center_device_top_menu_option_camera_uploads">Camera uploads</string>
    <!-- Label of the top app bar menu option of selected device in Device Center which opens the dialog to rename the selected Device -->
    <string name="device_center_device_top_menu_option_rename">Rename</string>
    <!-- Label of the top app bar menu option of selected device in Device Center which takes the user to the device info screen -->
    <string name="device_center_device_top_menu_option_info">Info</string>
    <!-- Text to show in the Device Center for a No Network State when there is no network connectivity -->
    <string name="device_center_no_network_state">No internet connection</string>
    <!-- Text to display in Device Center for an empty state when inspecting a device where no sync or backup has been configured -->
    <string name="device_center_nothing_setup_state">Nothing set up yet</string>
    <!-- The hint text for the search bar in the Device Center feature when the User is searching for Devices -->
    <string name="device_center_top_app_bar_search_devices_hint">Search devices</string>
    <!-- The hint text for the search bar in the Device Center feature when the User is searching for Syncs -->
    <string name="device_center_top_app_bar_search_syncs_hint">Search syncs</string>
    <!-- Text to show in the Device Center when no search results are found -->
    <string name="device_center_empty_screen_no_results">No results</string>
</resources>