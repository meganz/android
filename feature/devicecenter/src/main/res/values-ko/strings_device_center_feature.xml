    <string name="device_center_snackbar_message_rename_device_successful">장치 이름 변경됨</string>
    <!-- The Error Message shown in the Rename Device Dialog when the User submits a new Device Name whose length is greater than 32 characters -->
    <string name="device_center_rename_device_dialog_error_message_maximum_character_length_exceeded">최대 32자</string>
    <!-- Label of the top app bar menu option of selected device in Device Center which takes the user to the Camera uploads feature settings -->
    <string name="device_center_device_top_menu_option_camera_uploads">카메라 업로드</string>
