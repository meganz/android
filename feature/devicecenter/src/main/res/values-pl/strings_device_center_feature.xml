    <!-- The name of the Device or Folder Sub State when its code is NO_SYNC_ERROR -->
    <string name="device_center_list_view_item_sub_state_no_sync_error">No sync error</string>
    <!-- The name of the Device or Folder Sub State when its code is UNSUPPORTED_FILE_SYSTEM -->
    <string name="device_center_list_view_item_sub_state_unsupported_file_system">MEGA nie może zsynchronizować ani wykonać kopii zapasowej tego katalogu, ponieważ system plików na urządzeniu nie jest obsługiwany.</string>
    <!-- The name of the Device or Folder Sub State when its code is INVALID_REMOTE_TYPE -->
    <string name="device_center_list_view_item_sub_state_invalid_remote_type">Nie można zsynchronizować wybranego katalogu</string>
    <!-- The name of the Device or Folder Sub State when its code is INVALID_LOCAL_TYPE -->
    <string name="device_center_list_view_item_sub_state_invalid_local_type">Nie można zsynchronizować pliku pojedynczo. Należy skonfigurować synchronizację z katalogu.</string>
    <!-- The name of the Device or Folder Sub State when its code is INITIAL_SCAN_FAILED -->
    <string name="device_center_list_view_item_sub_state_initial_scan_failed">Wstępne skanowanie nie powiodło się. Konieczne będzie ponowne włączenie synchronizacji lub kopii zapasowej w aplikacji Desktop.</string>
    <!-- The name of the Device or Folder Sub State when its code is REMOTE_NODE_NOT_FOUND -->
    <string name="device_center_list_view_item_sub_state_remote_node_not_found">Katalog w MEGA nie może zostać zlokalizowany, ponieważ został przeniesiony lub usunięty, lub możesz nie mieć do niego dostępu.</string>
    <!-- The name of the Device or Folder Sub State when its code is STORAGE_OVERQUOTA -->
    <string name="device_center_list_view_item_sub_state_storage_overquota">Osiągnięto limit przydziału pamięci.</string>
    <!-- The name of the Device or Folder Sub State when its code is ACCOUNT_EXPIRED -->
    <string name="device_center_list_view_item_sub_state_account_expired">Nie można zsynchronizować lub utworzyć kopii zapasowej tego katalogu, ponieważ plan wygasł.</string>
    <!-- The name of the Device or Folder Sub State when its code is FOREIGN_TARGET_OVERSTORAGE -->
    <string name="device_center_list_view_item_sub_state_foreign_target_overshare">Nie można zsynchronizować katalogu, ponieważ użytkownik, który udostępnił ten katalog, osiągnął swój pojemność.</string>
    <!-- The name of the Device or Folder Sub State when its code is REMOTE_PATH_HAS_CHANGED -->
    <string name="device_center_list_view_item_sub_state_remote_path_has_changed">Katalog w MEGA nie może zostać zlokalizowany, ponieważ został przeniesiony lub usunięty, lub możesz nie mieć do niego dostępu.</string>
    <!-- The name of the Device or Folder Sub State when its code is SHARE_NON_FULL_ACCESS -->
    <string name="device_center_list_view_item_sub_state_share_non_full_access">Katalog nie może być zsynchronizowany, ponieważ jest to katalog współdzielony bez pełnego dostępu.</string>
    <!-- The name of the Device or Folder Sub State when its code is PUT_NODES_ERROR -->
    <string name="device_center_list_view_item_sub_state_put_nodes_error">Plików w tym katalogu nie można synchronizować ani tworzyć ich kopii zapasowych. Konieczne będzie ponowne włączenie synchronizacji lub tworzenia kopii zapasowych w aplikacji Desktop.</string>
    <!-- The name of the Device or Folder Sub State when its code is ACTIVE_SYNC_BELOW_PATH -->
    <string name="device_center_list_view_item_sub_state_active_sync_below_path">Katalog nie może być zsynchronizowany, ponieważ zawiera już zsynchronizowane katalogi.</string>
    <!-- The name of the Device or Folder Sub State when its code is VBOXSHAREDFOLDER_UNSUPPORTED -->
    <string name="device_center_list_view_item_sub_state_vboxsharedfolder_unsupported">MEGA nie może synchronizować ani tworzyć kopii zapasowych katalogów VirtualBox.</string>
    <!-- The name of the Device or Folder Sub State when its code is ACCOUNT_BLOCKED -->
    <string name="device_center_list_view_item_sub_state_account_blocked">Nie można zsynchronizować lub utworzyć kopii zapasowej tego katalogu, ponieważ konto zostało zablokowane.</string>
    <!-- The name of the Device or Folder Sub State when its code is UNKNOWN_TEMPORARY_ERROR -->
    <string name="device_center_list_view_item_sub_state_unknown_temporary_error">Problem z synchronizacją lub kopią zapasową tego katalogu. Spróbuj ponownie później. Jeśli problem będzie się powtarzał, skontaktuj się z pomocą techniczną.</string>
    <!-- The name of the Device or Folder Sub State when its code is TOO_MANY_ACTION_PACKETS -->
    <string name="device_center_list_view_item_sub_state_too_many_action_packets">Konto zostało przeładowane. Wszelkie pominięte aktualizacje kopii zapasowych lub synchronizacji nie zostały zastosowane.</string>
    <!-- The name of the Device or Folder Sub State when its code is LOGGED_OUT -->
    <string name="device_center_list_view_item_sub_state_logged_out">Synchronizacja lub tworzenie kopii zapasowej zostały zatrzymane, ponieważ wygląda na to, że użytkownik wylogował się z aplikacji Desktop. Zaloguj się ponownie za pomocą aplikacji Desktop i wznów synchronizację lub tworzenie kopii zapasowej.</string>
    <!-- The name of the Device or Folder Sub State when its code is MISSING_PARENT_NODE -->
    <string name="device_center_list_view_item_sub_state_missing_parent_node">N/A</string>
    <!-- The name of the Device or Folder Sub State when its code is BACKUP_SOURCE_NOT_BELOW_DRIVE -->
    <string name="device_center_list_view_item_sub_state_backup_source_not_below_drive">Nie można zlokalizować katalogu na dysku zewnętrznym.</string>
    <!-- The name of the Device or Folder Sub State when its code is ACTIVE_SYNC_SAME_PATH -->
    <string name="device_center_list_view_item_sub_state_active_sync_same_path">W tej samej ścieżce znajduje się już zsynchronizowany katalog.</string>
    <!-- The name of the Device or Folder Sub State when its code is COULD_NOT_MOVE_CLOUD_NODES -->
    <string name="device_center_list_view_item_sub_state_could_not_move_cloud_nodes">Zmiana nazwy nie powiodła się.</string>
    <!-- The name of the Device or Folder Sub State when its code is COULD_NOT_CREATE_IGNORE_FILE -->
    <string name="device_center_list_view_item_sub_state_could_not_create_ignore_file">Nie można utworzyć pliku .megaignore dla tej synchronizacji.</string>
    <!-- The name of the Device or Folder Sub State when its code is SYNC_CONFIG_READ_FAILURE -->
    <string name="device_center_list_view_item_sub_state_sync_config_read_failure">Nie można odczytać konfiguracji synchronizacji. Spróbuj ponownie później lub sprawdź uprawnienia katalogu.</string>
    <!-- The name of the Device or Folder Sub State when its code is UNKNOWN_DRIVE_PATH -->
    <string name="device_center_list_view_item_sub_state_unknown_drive_path">Ścieżka dysku synchonizacji jest nieznana.</string>
    <!-- The name of the Device or Folder Sub State when its code is INVALID_SCAN_INTERVAL -->
    <string name="device_center_list_view_item_sub_state_invalid_scan_interval">Nieprawidłowy interwał skanowania. Sprawdź ustawienia interwału skanowania i spróbuj ponownie.</string>
    <!-- The name of the Device or Folder Sub State when its code is NOTIFICATION_SYSTEM_UNAVAILABLE -->
    <string name="device_center_list_view_item_sub_state_notification_system_unavailable">Nie można nawiązać komunikacji z lokalizacją katalogu. Sprawdź, czy lokalizacja jest dostępna i czy przyznano uprawnienia do lokalizacji katalogu....</string>
    <!-- The name of the Device or Folder Sub State when its code is UNABLE_TO_ADD_WATCH -->
    <string name="device_center_list_view_item_sub_state_unable_to_add_watch">Nie można dodać obserwacji systemu plików. Upewnij się, że jest wystarczająco dużo wolnego miejsca i pamięci oraz że masz uprawnienia do lokalizacji katalogu.</string>
    <!-- The name of the Device or Folder Sub State when its code is UNABLE_TO_RETRIEVE_ROOT_FSID -->
    <string name="device_center_list_view_item_sub_state_unable_to_retrieve_root_fsid">Nie można odczytać lokalizacji synchronizacji. Sprawdź, czy lokalizacja jest dostępna i czy przyznano uprawnienia do lokalizacji katalogu.</string>
    <!-- The name of the Device or Folder Sub State when its code is UNABLE_TO_OPEN_DATABASE -->
    <string name="device_center_list_view_item_sub_state_unable_to_open_database">Unable to open state cache database</string>
    <!-- The name of the Device or Folder Sub State when its code is INSUFFICIENT_DISK_SPACE -->
    <string name="device_center_list_view_item_sub_state_insufficient_disk_space">Niewystarczająca ilość miejsca do pobrania.</string>
    <!-- The name of the Device or Folder Sub State when its code is FAILURE_ACCESSING_PERSISTENT_STORAGE -->
    <string name="device_center_list_view_item_sub_state_failure_accessing_persistent_storage">Nie można odczytać lokalizacji synchronizacji. Sprawdź, czy lokalizacja jest dostępna i czy przyznano uprawnienia do lokalizacji katalogu.</string>
    <!-- The name for specific Sub States to indicate that an unknown error occurred -->
    <string name="device_center_list_view_item_sub_state_message_unknown_error">Nieznany błąd.</string>
    <!-- The name for specific Sub States to indicate that something went wrong -->
    <string name="device_center_list_view_item_sub_state_message_something_went_wrong">Something went wrong.</string>
    <!-- The name for specific Sub States to indicate that the Node is in the Rubbish Bin -->
    <string name="device_center_list_view_item_sub_state_message_node_in_rubbish_bin">Katalogu nie można zsynchronizować ani wykonać kopii zapasowej, ponieważ MEGA katalog znajduje się w Koszu.</string>
    <!-- The name for specific Sub States to indicate that the local drive cannot be located at this time -->
    <string name="device_center_list_view_item_sub_state_message_cannot_locate_local_drive_now">Nie można teraz zlokalizować katalogu w urządzeniu, spróbuj ponownie później.</string>
    <!-- The name for specific Sub States to indicate that the local drive cannot be located -->
    <string name="device_center_list_view_item_sub_state_message_cannot_locate_local_drive">Nie można zlokalizować katalogu w urządzeniu</string>
    <!-- The name for specific Sub States to indicate that a problem occurred when syncing or backing up the MEGA Folder due to some changes -->
    <string name="device_center_list_view_item_sub_state_message_folder_backup_issue_due_to_recent_changes">Problem z synchronizacją lub tworzeniem kopii zapasowej tego katalogu z powodu zmian w katalogu MEGA. Zatrzymaj synchronizację lub kopię zapasową i spróbuj ponownie skonfigurować ją w aplikacja desktopowa komputerowej lub kontakt techniczną.</string>
    <!-- The name for specific Sub States to indicate that a problem occurred when syncing or backing up the MEGA Folder -->
    <string name="device_center_list_view_item_sub_state_message_folder_backup_issue">Problem z synchronizacją lub kopią zapasową tego katalogu. Zatrzymaj synchronizację lub tworzenie kopii zapasowej i spróbuj skonfigurować je ponownie w aplikacji Desktop lub skontaktuj się z pomocą techniczną.</string>
    <!-- The name for specific Sub States to indicate that the MEGA Folder cannot be synced or backed up as it is inside another synced MEGA Folder -->
    <string name="device_center_list_view_item_sub_state_message_folder_backup_issue_due_to_being_inside_another_backed_up_folder">Katalog nie może być zsynchronizowany, ponieważ znajduje się już w zsynchronizowanym katalogu.</string>
    <!-- The name of the Device Status informing the User that Camera Uploads is disabled for the Current Device -->
    <string name="device_center_list_view_item_status_camera_uploads_disabled">Przesyłanie z kamery wyłączone</string>
    <!-- The Error Message shown in the Rename Device Dialog when the User submits an empty Device Name -->
    <string name="device_center_rename_device_dialog_error_message_empty_device_name">Wprowadź nazwę urządzenia.</string>
    <!-- The Error Message shown in the Rename Device Dialog when the User submits a new Device Name that already exists in the current list of Devices -->
    <string name="device_center_rename_device_dialog_error_message_name_already_exists">Urządzenie o tej nazwie już istnieje. Wprowadź inną nazwę.</string>
    <!-- The Error Message shown in the Rename Device Dialog when the User submits a new Device Name with invalid characters -->
    <string name="device_center_rename_device_dialog_error_message_invalid_characters">Następujące znaki są niedozwolone: %1$s</string>
    <!-- The Snackbar message to inform the User that the Device has been successfully renamed -->
    <string name="device_center_snackbar_message_rename_device_successful">Zmieniono nazwę urządzenia</string>
    <!-- The Error Message shown in the Rename Device Dialog when the User submits a new Device Name whose length is greater than 32 characters -->
    <string name="device_center_rename_device_dialog_error_message_maximum_character_length_exceeded">Maksymalnie 32 znaki</string>
    <!-- Label of the top app bar menu option of selected device in Device Center which takes the user to the Camera uploads feature settings -->
    <string name="device_center_device_top_menu_option_camera_uploads">Przesyłanie z kamery</string>
    <!-- Label of the top app bar menu option of selected device in Device Center which opens the dialog to rename the selected Device -->
    <string name="device_center_device_top_menu_option_rename">Zmień nazwę</string>
    <!-- Label of the top app bar menu option of selected device in Device Center which takes the user to the device info screen -->
    <string name="device_center_device_top_menu_option_info">Info</string>
    <!-- Text to show in the Device Center for a No Network State when there is no network connectivity -->
    <string name="device_center_no_network_state">Brak połączenia z internetem</string>
    <!-- The name of the Device or Folder Sub State when its code is LOCAL_FILESYSTEM_MISMATCH -->
    <string name="device_center_list_view_item_sub_state_local_filesystem_mismatch">Plików w tym katalogu nie można synchronizować ani tworzyć ich kopii zapasowych. Konieczne będzie ponowne włączenie synchronizacji lub tworzenia kopii zapasowych w aplikacji Desktop.</string>
    <!-- Text to display in Device Center for an empty state when inspecting a device where no sync or backup has been configured -->
    <string name="device_center_nothing_setup_state">Nic nie zostało jeszcze skonfigurowane</string>
    <!-- The hint text for the search bar in the Device Center feature when the User is searching for Devices -->
    <string name="device_center_top_app_bar_search_devices_hint">Wyszukiwanie urządzeń</string>
    <!-- The hint text for the search bar in the Device Center feature when the User is searching for Syncs -->
    <string name="device_center_top_app_bar_search_syncs_hint">Synchronizacja wyszukiwania</string>
